<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>

<VBox alignment="CENTER" prefHeight="144.0" prefWidth="211.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.oskopek.studyguide.controller.FindCoursesController">
    <children>
        <Label text="%studyPane.addCourse">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TextField>
            <VBox.margin>
                <Insets bottom="-10.0" />
            </VBox.margin>
        </TextField>
        <Button mnemonicParsing="false" text="%studyPane.search">
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </Button>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
