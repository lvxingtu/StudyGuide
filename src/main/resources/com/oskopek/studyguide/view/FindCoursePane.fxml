<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" prefHeight="144.0" prefWidth="211.0" spacing="20.0"
      xmlns="http://javafx.com/javafx/8.0.40" fx:controller="com.oskopek.studyguide.controller.FindCoursesController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Label text="%studyPane.addCourse">
        <font>
            <Font size="18.0"/>
        </font>
    </Label>
    <TextField fx:id="searchField" onAction="#handleSearch">
        <VBox.margin>
            <Insets bottom="-10.0"/>
        </VBox.margin>
    </TextField>
    <Button fx:id="searchButton" mnemonicParsing="false" text="%studyPane.search" onAction="#handleSearch">
        <VBox.margin>
            <Insets/>
        </VBox.margin>
    </Button>
</VBox>